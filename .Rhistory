}
par(mar=c(2,2,2,2))
plot(df[df$siteName==i,]$Date, df[df$siteName==i,]$PM2.5, type = "l",main = i, xlab = "", ylab = "PM2.5")
plot(df[df$siteName==i-6,]$Date, df[df$siteName==i,]$PM2.5, type = "l",main = i, xlab = "", ylab = "PM2.5")
plot(df[df$siteName=="Narela",]$Date, df[df$siteName=="Narela",]$PM2.5, type = "l",main = i, xlab = "", ylab = "PM2.5")
